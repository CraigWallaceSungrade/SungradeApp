<p id="notice"><%= notice %></p>

<h1>Appointment Staffs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @appointment_staffs.each do |appointment_staff| %>
      <tr>
        <td><%= link_to 'Show', appointment_staff %></td>
        <td><%= link_to 'Edit', edit_appointment_staff_path(appointment_staff) %></td>
        <td><%= link_to 'Destroy', appointment_staff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

