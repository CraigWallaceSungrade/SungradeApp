<p id="notice"><%= notice %></p>

<h1>Property Staffs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @property_staffs.each do |property_staff| %>
      <tr>
        <td><%= link_to 'Show', property_staff %></td>
        <td><%= link_to 'Edit', edit_property_staff_path(property_staff) %></td>
        <td><%= link_to 'Destroy', property_staff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property Staff', new_property_staff_path %>
